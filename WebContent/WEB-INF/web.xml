<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Rent_It</display-name>
  <welcome-file-list>
    <welcome-file>search</welcome-file>
    <welcome-file>Register.jsp</welcome-file>
    <welcome-file>Logout.jsp</welcome-file>
    <welcome-file>EditProfile.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.rentit.webapp.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>registerUser</servlet-name>
    <servlet-class>com.rentit.webapp.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>registerUser</servlet-name>
    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>fetchProducts</servlet-name>
    <servlet-class>com.rentit.webapp.FetchProductsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>fetchProducts</servlet-name>
    <url-pattern>/FetchProductsServlet</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>postItem</servlet-name>
    <servlet-class>com.rentit.webapp.PostItemServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>postItem</servlet-name>
    <url-pattern>/PostItemServlet</url-pattern>
  </servlet-mapping>
</web-app>